{
  "name": "mmbaydin",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --https",
    "lint": "eslint --ext .jsx --ext .tsx --ext .ts --ext .js --ext .test.js ./",
    "lint:fix": "eslint --fix --ext .jsx --ext .tsx --ext .ts --ext .js --ext .test.js ./",
    "type-check": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check ./src/",
    "test": "jest",
    "test:coverage": "jest --clear-cache && jest --coverage --logHeapUsage --maxConcurrency=4 --detectOpenHandles --runInBand",
    "build-android:dev": "eas build -p android --profile development",
    "build-android:prod": "eas build -p android --profile production",
    "build-android:release": "eas build -p android --profile release",
    "build-ios:dev": "eas build -p ios --profile development",
    "build-ios:prod": "eas build -p ios --profile production",
    "build-ios:release": "eas build -p ios --profile release",
    "submit-android": "",
    "submit-ios": "",
    "ota-release": "eas update --branch release --auto",
    "build-web": "npx expo export:web",
    "run-build-web": "yarn build-web && npx serve web-build"
  },
  "dependencies": {
    "@expo/config": "^8.0.2",
    "@expo/config-plugins": "~6.0.0",
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "expo": "~48.0.6",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-font": "~11.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "firebase": "^9.19.0",
    "jest-expo": "^48.0.2",
    "jest-fetch-mock": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-paper": "^5.5.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "resize-observer-polyfill": "^1.5.1",
    "twrnc": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "lefthook": "^1.3.6",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "private": true
}
